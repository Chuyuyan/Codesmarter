{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Extension",
            "type": "extensionHost",
            "request": "launch",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ],
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "preLaunchTask": "${defaultBuildTask}",
            "sourceMaps": true,
            "runtimeExecutable": "${execPath}",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}

